{
    "version": "2.0",
    "extensions": {
        "serviceBus": {
          "messageHandlerOptions": {
            "autoComplete": true,
            "maxConcurrentCalls": 16,
            "maxAutoRenewDuration": "00:05:00"
          },
          "prefetchCount": 0,
          "transportType": "amqpTcp"
        }
      },
      "eventHubs": {
        "batchCheckpointFrequency": 1,
        "eventProcessorOptions": {
          "maxBatchSize": 50
        }
      },
    "extensionBundle": {
          "id": "Microsoft.Azure.Functions.ExtensionBundle",
          "version": "[4.0.0, 5.0.0)"
    }
}